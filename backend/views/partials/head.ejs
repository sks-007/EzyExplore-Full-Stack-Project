  <style>
    #learn-more-btn {
      background-color: white;
    }

    .filter-tab {
      color: #333;
      background-color: transparent;
      border: none;
      cursor: pointer;
      transition: color 0.3s ease;
    }

    .filter-tab:hover {
      color: white;
    }

    .filter-tab i,
    .filter-tab span {
      transition: color 0.3s ease;
    }

    .filter-tab:hover i,
    .filter-tab:hover span {
      color: white;
    }
  </style>
